{
  "name": "@ryanwilldev/react-calendario",
  "version": "1.1.1",
  "description": "A flexible and customizable primitive React component for creating internationalized calendars and date pickers",
  "main": "dist/react-calendario.js",
  "types": "dist/react-calendario.d.ts",
  "scripts": {
    "build": "npm run clean:all && mkdir ./dist && npm run compile && npm run bundle && npm run clean:compiled",
    "bundle": "node ./build-bundle.js",
    "compile": "tsc",
    "clean:all": "rimraf ./dist && rimraf ./compiled",
    "clean:compiled": "rimraf ./compiled",
    "test": "jest",
    "test:continuous": "jest --watch",
    "uglify": "uglify-js ./dist/index.js",
    "watch": "tsc --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RyanWillDev/react-calendario.git"
  },
  "keywords": [
    "react",
    "calendar",
    "date",
    "picker",
    "i18n",
    "internationalization"
  ],
  "author": "Ryan Will https://www.ryanwilldev.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RyanWillDev/react-calendario/issues"
  },
  "homepage": "https://github.com/RyanWillDev/react-calendario#readme",
  "devDependencies": {
    "@types/jest": "^23.0.0",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "jest": "^23.1.0",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-testing-library": "^3.1.4",
    "rimraf": "^2.6.2",
    "rollup": "^0.60.2",
    "ts-jest": "^22.4.6",
    "typescript": "^2.8.3"
  },
  "peerDependencies": {
    "react": ">=0.14.x"
  },
  "dependencies": {
    "calendar-base": "^0.3.0"
  },
  "files": [
    "dist/"
  ]
}
